#!/usr/bin/expect

set home [lindex $argv 0]
set from [lindex $argv 1]
set chain_id [lindex $argv 2]
set amount [lindex $argv 3]
set to [lindex $argv 4]

set timeout 30
	spawn ./bnbcli token transfer --home $home --from $from --chain-id=$chain_id --amount $amount --to $to
	expect "Password*"
	send "12345678\r"
interact
