#!/usr/bin/expect

set home [lindex $argv 0]
set from [lindex $argv 1]
set chain_id [lindex $argv 2]
set tx [lindex $argv 3]

set timeout 30
	spawn ./bnbcli token multi-send --home $home --from $from --chain-id=$chain_id --transfers $tx
	expect "confirm transaction before signing and broadcasting*"
	send "y\r"
	expect "Password*"
	send "12345678\r"
interact
